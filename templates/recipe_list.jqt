{{each recipes}}
  <li data-recipe-id="${$value.id}" class="clickable">
    <table style="width:100%">
      <tr>
        <td><img class="recipe-image" height="48" width="48" src="${recipeURL($value.image_thumb)}"></td>
        <td>
          ${$value.name}
        </td>
      </tr>
    </table>
  </li>
{{/each}}